#include <stdio.h>
using namespace std;

int main(int argc, char *argv[]) {
	int n;
	int matriz[2][2];
	scanf("%d",&n);
	while(n--){
		scanf("%d",&matriz[0][0]);
		scanf("%d",&matriz[0][1]);
		scanf("%d",&matriz[1][0]);
		scanf("%d",&matriz[1][1]);
		printf("%d ",matriz[0][0]);
		printf("%d\n",matriz[0][1]);
		printf("%d ",matriz[1][0]);
		printf("%d\n",matriz[1][1]);
	}
	return 0;
}

